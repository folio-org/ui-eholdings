{
  "name": "@folio/eholdings",
  "version": "0.1.0",
  "description": "FOLIO UI module for eHoldings",
  "main": "src/index.js",
  "repository": "https://github.com/folio-org/ui-eholdings",
  "license": "Apache-2.0",
  "scripts": {
    "start": "NODE_ENV=development stripescli serve demo/stripes.config.js",
    "build": "stripescli build demo/stripes.config.js dist",
    "test": "stripescli test demo/stripes.config.js --type=unit",
    "lint": "eslint ./"
  },
  "devDependencies": {
    "@folio/stripes-cli": "folio-org/stripes-cli#master",
    "@folio/stripes-components": "folio-org/stripes-components#master",
    "@folio/stripes-core": "folio-org/stripes-core#master",
    "babel-plugin-istanbul": "^4.1.5",
    "chai": "^4.0.2",
    "chai-jquery": "^2.0.0",
    "eslint": "^4.8.0",
    "eslint-config-stripes": "^1.0.0",
    "eslint-loader": "^1.9.0",
    "jquery": "^3.2.1",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "lodash": "^4.17.4",
    "mirage-server": "cowboyd/mirage-server",
    "moment": "^2.17.1",
    "react-hot-loader": "next",
    "react-trigger-change": "^1.0.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "impagination": "^1.0.0-alpha.3",
    "inflected": "^2.0.3",
    "isomorphic-fetch": "^2.2.1",
    "query-string": "^5.0.0",
    "redux-actions": "^2.2.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "stripes": {
    "type": "app",
    "displayName": "eHoldings",
    "route": "/eholdings",
    "hasSettings": true,
    "okapiInterfaces": {
      "eholdings": "0.1"
    },
    "permissionSets": [
      {
        "permissionName": "module.eholdings.enabled",
        "displayName": "UI: eHoldings module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.eholdings.enabled",
        "displayName": "Settings (eHoldings): display list of settings pages",
        "visible": true
      }
    ]
  }
}
