    "eslint-loader": "^1.9.0",
    "file-loader": "^1.1.5",
    "jquery": "^3.2.1",
    "karma": "^2.0.4",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
