{
  "name": "@folio/eholdings",
  "version": "0.1.0",
  "description": "FOLIO UI module for eHoldings",
  "main": "src/index.js",
  "repository": "https://github.com/folio-org/ui-eholdings",
  "license": "Apache-2.0",
  "scripts": {
    "start": "stripes serve demo/stripes.config.js",
    "build": "stripes build demo/stripes.config.js dist",
    "test": "stripes test karma demo/stripes.config.js",
    "lint": "eslint ./ && stylelint \"src/**/*.css\"",
    "eslint": "eslint ./",
    "stylelint": "stylelint \"src/**/*.css\""
  },
  "devDependencies": {
    "@bigtest/convergence": "^0.7.0",
    "@bigtest/interactor": "^0.4.2",
    "@bigtest/mirage": "^0.0.1",
    "@bigtest/mocha": "^0.3.3",
    "@folio/eslint-config-stripes": "^1.1.0",
    "@folio/stripes-cli": "^1.1.0",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-remove-jsx-attributes": "^0.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "chai": "^4.0.2",
    "chai-jquery": "^2.0.0",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "file-loader": "^1.1.5",
    "jquery": "^3.2.1",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-junit-reporter": "^1.2.0",
    "karma-webpack": "^3.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "qs": "^6.5.1",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-hot-loader": "next",
    "react-transition-group": "^2.2.1",
    "react-trigger-change": "^1.0.2",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "@folio/stripes-components": "^2.0.18000345",
    "classnames": "^2.2.5",
    "funcadelic": "^0.4.0",
    "impagination": "^1.0.0-alpha.3",
    "inflected": "^2.0.3",
    "isomorphic-fetch": "^2.2.1",
    "redux-actions": "^2.2.1"
  },
  "peerDependencies": {
    "react": "*"
  },
  "stripes": {
    "type": "app",
    "displayName": "eHoldings",
    "route": "/eholdings",
    "hasSettings": true,
    "icons": [
      {
        "name": "app",
        "alt": "FOLIO UI module for eHoldings",
        "title": "eHoldings"
      }
    ],
    "okapiInterfaces": {
      "eholdings": "0.1"
    },
    "permissionSets": [
      {
        "permissionName": "module.eholdings.enabled",
        "displayName": "UI: eHoldings module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.eholdings.enabled",
        "displayName": "Settings (eHoldings): display list of settings pages",
        "visible": true
      }
    ]
  }
}
