{
  "name": "@folio/eholdings",
  "version": "1.0.1",
  "description": "FOLIO UI module for eHoldings",
  "main": "src/index.js",
  "repository": "https://github.com/folio-org/ui-eholdings",
  "license": "Apache-2.0",
  "scripts": {
    "start": "stripes serve",
    "build": "stripes build --output dist",
    "test": "stripes test karma",
    "lint": "eslint ./ && stylelint \"src/**/*.css\"",
    "eslint": "eslint ./",
    "stylelint": "stylelint \"src/**/*.css\""
  },
  "devDependencies": {
    "@bigtest/convergence": "^0.10.0",
    "@bigtest/interactor": "^0.7.2",
    "@bigtest/mirage": "^0.0.1",
    "@bigtest/mocha": "^0.5.1",
    "@bigtest/react": "^0.1.2",
    "@folio/eslint-config-stripes": "frontside-folio/eslint-config-stripes#jc/webpack-dependency",
    "@folio/stripes-cli": "^1.4.0",
    "@folio/stripes-core": "^2.12.0",
    "babel-eslint": "^9.0.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.0.2",
    "chai-jquery": "^2.0.0",
    "eslint": "^5.5.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "mocha": "^5.2.0",
    "moment": "^2.22.2",
    "prop-types": "^15.6.1",
    "qs": "^6.5.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-transition-group": "^2.2.1",
    "react-trigger-change": "^1.0.2",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-junit-formatter": "^0.2.1"
  },
  "dependencies": {
    "@folio/stripes-components": "^3.1.0",
    "classnames": "^2.2.5",
    "funcadelic": "^0.5.4",
    "impagination": "^1.0.0-alpha.3",
    "inflected": "^2.0.3",
    "react-intl": "^2.4.0",
    "react-measure": "2.1.0",
    "redux-actions": "^2.2.1"
  },
  "peerDependencies": {
    "@folio/stripes-core": "^2.12.0",
    "react": "*"
  },
  "stripes": {
    "type": "app",
    "displayName": "ui-eholdings.meta.title",
    "route": "/eholdings",
    "hasSettings": true,
    "icons": [
      {
        "name": "app",
        "alt": "FOLIO UI module for eHoldings",
        "title": "eHoldings"
      }
    ],
    "okapiInterfaces": {
      "eholdings": "0.1"
    },
    "permissionSets": [
      {
        "permissionName": "module.eholdings.enabled",
        "displayName": "UI: eHoldings module is enabled",
        "visible": true
      },
      {
        "permissionName": "settings.eholdings.enabled",
        "displayName": "Settings (eHoldings): display list of settings pages",
        "visible": true
      }
    ]
  }
}
